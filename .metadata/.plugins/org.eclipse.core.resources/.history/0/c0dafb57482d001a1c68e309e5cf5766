#port and context path
  
server:
  port: 8083
  servlet:
    context-path: /auth
    
    
spring :
  thymeleaf:
    cache: false
    check-template: true
    check-template-location: true
    enabled: true
    mode: HTML
    prefix: classpath:/templates/
    suffix: .html
    servlet:
      content-type: text/html